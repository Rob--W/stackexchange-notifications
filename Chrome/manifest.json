{
    "name": "Desktop Notifications for Stack Exchange",
    "description": "Real-time desktop notifications for the Stack Exchange.",
    "version": "1.5.2",
    "manifest_version": 2,
    "background": {
        "scripts": ["using-websocket.js"]
    },
    "options_page": "options.html",
    "optional_permissions": [
        "background"
    ],
    "permissions": [
        "notifications",
        "tabs"
    ]
}
